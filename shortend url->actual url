import requests
from bs4 import BeautifulSoup

def get_actual_url(masked_url):
    response = requests.get(masked_url)
    soup = BeautifulSoup(response.content, 'html.parser')
    # Look for common frame or meta refresh tags
    iframe = soup.find('iframe')
    if iframe:
        return iframe['src']
    meta_refresh = soup.find('meta', attrs={"http-equiv": "refresh"})
    if meta_refresh:
        return meta_refresh['content'].split('url=')[-1]
    return response.url  # fallback to the final URL if no masking is found

masked_url= input("Please enter something: ")
actual_url = get_actual_url(masked_url)
print(f"The actual URL is: {actual_url}")
